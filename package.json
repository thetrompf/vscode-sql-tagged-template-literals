{
  "name": "vscode-sql-tagged-template-literals",
  "displayName": "SQL tagged template literals",
  "description": "Syntax highlighting for SQL in template literals tagged with `sql` function",
  "version": "1.0.0",
  "license": "MIT",
  "publisher": "thetrompf",
  "repository": {
    "type": "git",
    "url": "https://github.com/thetrompf/vscode-sql-tagged-template-literals"
  },
  "engines": {
    "vscode": "^1.34.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "grammars": [
      {
        "injectTo": [
          "source.js",
          "source.js.jsx",
          "source.jsx",
          "source.ts",
          "source.tsx"
        ],
        "scopeName": "inline.tagged-template-sql",
        "path": "./syntaxes/grammar.json",
        "embeddedLanguages": {
          "meta.embedded.block.sql": "sql"
        }
      }
    ]
  }
}
